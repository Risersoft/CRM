                        </div>
                        <div id="collapse2" class="panel-collapse collapse show" role="tabpanel" aria-labelledby="heading2">
                            <div class="panel-body">
                                <div class="col-md-12">
                                    <div Class="row">
                                        <div Class="col-md-1"></div>

                                        <div class="col-md-4">
                                            <label class="control-label labeltext">Address <span class="red">*</span></label>
                                            <textarea name="cadd" ng-model="CompanyInfo.Address" class="form-control" style="height:113px;resize:none;" required ng-class="{true: 'error'}[submitted() && userform.cadd.$invalid]"></textarea>
                                        </div>
                                        <div class="col-md-7">
                                            <div class="row">
                                                <div Class="col-md-5">
                                                    <Label Class="control-label labeltext">City <span class="red">*</span></Label>
                                                    <input name="city" type="text" ng-model="CompanyInfo.City" Class="form-control" required ng-class="{true: 'error'}[submitted() && userform.city.$invalid]" />
                                                </div>
                                                <div class="col-md-5">
                                                    <Label Class="control-label labeltext">PinCode <span class="red">*</span></Label>
                                                    <input name="pcode" type="text" ng-model="CompanyInfo.Pincode" Class="form-control" maxlength="6" numbers-only required ng-class="{true: 'error'}[submitted() && userform.pcode.$invalid]" />
                                                </div>

                                            </div>
                                            <div class="row">
                                                <div Class="col-md-5">
                                                    <Label Class="control-label labeltext">Country <span class="red">*</span></Label>
                                                    <select name="country" ng-model="CompanyInfo.CountrySelected" ng-options="itemcoun.country for itemcoun in dsCombo.CO track by itemcoun.country" ng-change="changestate(CompanyInfo.CountrySelected)" Class="form-control" required ng-class="{true: 'error'}[submitted() && userform.country.$invalid]"></select>

                                                </div>
                                                <div Class="col-md-5">
                                                    <Label Class="control-label labeltext">State <span class="red">*</span></Label>
                                                    <select name="state" ng-model="CompanyInfo.StateSelected" ng-options="itemsu.SubDivisionName for itemsu in State track by itemsu.SubDivisionName" Class="form-control" required ng-class="{true: 'error'}[submitted() && userform.state.$invalid]"></select>
                                                </div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-1"></div>
                                        <div class="col-md-4">
                                            <label class="control-label labeltext">CIN No.</label>
                                            <input type="text" ng-model="CompanyInfo.CINNum" class="form-control" />
                                        </div>
                                        <div class="col-md-3">
                                            <label class="control-label labeltext">Ph No.</label>
                                            <label>
                                                <input type="text" ng-model="CompanyInfo.PhCountry" class="input-group-addon" style="width:20%;" />
                                                <input type="text" ng-model="CompanyInfo.PhArea" class="input-group-addon" style="width:20%" />
                                                <input type="text" ng-model="CompanyInfo.Phnum" class="input-group-addon" style="width:53%" />
                                            </label>
                                        </div>
                                        <div class="col-md-3">
                                            <label class="control-label labeltext">Fax No.</label>
                                            <label>
                                                <input type="text" ng-model="CompanyInfo.FaxCountry" class="input-group-addon" style="width:20%" />
                                                <input type="text" ng-model="CompanyInfo.FaxArea" class="input-group-addon" style="width:20%" />
                                                <input type="text" ng-model="CompanyInfo.Faxnum" class="input-group-addon" style="width:51%" />
                                            </label>
                                        </div>
                                    </div>

                                    <div class="row">
                                        <div class="col-md-1"></div>
                                        <div class="col-md-4">
                                            <label class="control-label labeltext">Email</label>
                                            <input type="text" ng-model="CompanyInfo.Email" class="form-control" />
                                        </div>
                                        <div class="col-md-3">
                                            <label class="control-label labeltext">Web</label>
                                            <input type="text" ng-model="CompanyInfo.Web" class="form-control" />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row"><hr /></div>
                </div>
            </div>



