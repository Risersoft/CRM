@section botscripts{
    <script>
        $('.panel-collapse').on('show.bs.collapse', function () {
            $(this).siblings('.panel-heading').addClass('active');
        });

        $('.panel-collapse').on('hide.bs.collapse', function () {
            $(this).siblings('.panel-heading').removeClass('active');
        });
    </script>
    <script type="text/javascript">


        $(document).ready(function () {
            setInterval(function () {
                $(".my-datepicker").removeClass("my-datepicker").each(function () {
                    $(this).datepicker({ dateFormat: 'yy-mm-dd' });
                });
            }, 10);
        });

        rsApp.controller('FormController', function ($scope, $http) {

            $scope.model = JSON.parse($("#model_json").val());
            $scope.status = 'loaded';
            var loadmodel = function (result) {
             // debugger;
                $scope.model = result;
                $scope.EnqItem = $scope.model.dsForm.DT[0];
                $scope.ValueLists = $scope.model.ValueLists;
                //debugger;
                $scope.dsCombo = $scope.model.dsCombo;
                $scope.SuperChType = $scope.model.ValueLists.SuperChType.ValueListItems;
                $scope.Service = $scope.model.GridViews.Service.MainGrid.myDS.dt;
                $scope.Spare = $scope.model.GridViews.Spare.MainGrid.myDS.dt;
                $scope.document = $scope.model.GridViews.Doc.MainGrid.myDS.dt;
                $scope.test = $scope.model.GridViews.TT.MainGrid.myDS.dt;
                $scope.quotes = $scope.model.GridViews.CQ.MainGrid.myDS.dt;
                $scope.Supplier = $scope.model.GridViews.CQ.MainGrid.dsLookup.Table0;
                $scope.ServiceType = $scope.model.GridViews.Service.MainGrid.dsLookup.Table0;
                $scope.Documenttype = $scope.model.GridViews.Doc.MainGrid.dsLookup.Table0;
                $scope.TTtype = $scope.model.GridViews.TT.MainGrid.dsLookup.Table0;
                $scope.visbacktoform = ($scope.model.frmMode == 2 ? false : true);
                $scope.tfddl = [{ value: true, name: 'Yes' }, { value: false, name: 'No' }];
                 @Html.RenderLookup("EnqItem", Model, Model.dsForm.Tables(0))
                       }
            loadmodel($scope.model)
            $scope.copyvalueonchange = function (val) {
                if (val == '1') {
                    $scope.QtyTBQ12 = $scope.EnqItem.QtyAPT2;
                }
                else if (val == '2') {
                    $scope.QtyExp13 = $scope.EnqItem.QtyAPT2;
                }
            }

            $scope.changeitem = function (key,value) {
                //debugger;
                if (key == 'name') {
                    $scope.itemnam = $.grep($scope.model.dsCombo.Items, function (item, index) { return item.itemid == value.itemid })[0];
                    $scope.Itemname = $scope.itemnam;
                }

                else if (key == 'code') {
                    //debugger;
                    $scope.itemcod= $.grep($scope.model.dsCombo.Items, function (item, index) { return item.itemid == value.itemid })[0];
                    $scope.EnqItem.ItemIDSelected = $scope.itemcod;
                }
            }

            //Add new row
            $scope.Add = function (val) {
                $scope[val].push({});
            }
           
            //delete row
            $scope.delete = function (index, val) {
                $scope[val].splice(index, 1);
            }

            $scope.backtoprev = function () {
               
                $scope.info = $scope.EnqItem;           
                $scope.salesenqid = $scope.model.dsForm.DT[0].SalesEnqID;             
                    var base = '/App/Link' + location.search;
                   // var payload = { fKey: 'frmSalesEnq', fMode: 'acAddM', IDX: 0, rowData: JSON.stringify($scope.info) } // fMode: 'acAddM',
                    var payload = { fKey: 'frmSalesEnq', fMode: 'acEditM', IDX: $scope.salesenqid, rowData: JSON.stringify($scope.info) }; // for edit
                    $.post(base, payload, function (result) {
                        $scope.status = 'loaded';
                        window.location = result;
                    })
             }
            $scope.cleanupmodel = function (datamodel) {
             //empty function
            };
            $scope.calculatemodel = function () {
                //empty function
              
                if ($scope.Itemname.IsETO) {
                    $scope.model.dsForm.DT[0].descripwo = $scope.EnqItem.ItemIDSelected.ETODescripAttribID;
                    $scope.model.dsForm.DT[0].descriptech = $scope.EnqItem.ItemIDSelected.ETOTechDescripAttribID;
                }
                else {
                    $scope.model.dsForm.DT[0].descripwo = $scope.EnqItem.ItemIDSelected.itemname;
                    $scope.model.dsForm.DT[0].descriptech = $scope.EnqItem.ItemIDSelected.itemname;
                }

            };

            $scope.visdel = false;
            @Html.RenderFile("~/scripts/rsforms.js");
        });
    </script> 
}






